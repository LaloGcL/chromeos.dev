{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "importsNotUsedAsValues": "error",
    "baseUrl": ".",
    "paths": {
      "$components/*": ["src/components/*"],
      "$layouts/*": ["src/layouts/*"],
      "$lib/*": ["lib/*"],
      "$types/*": ["types/*"],
      "$sass/*": ["src/sass/*"],
      "$markdown/*": ["src/markdown/*"],
      "$$sanity": ["lib/sanity.ts"],
      "$$markdown": ["lib/markdown.ts"]
    }
  }
}
